        "lint": "eslint src/rules --fix"
    },
    "dependencies": {
        "requireindex": "~1.1.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.2",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-0": "^6.24.1",
        "cross-env": "^5.1.4",
        "eslint": "^4.17.0",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-import": "^2.9.0",
        "mocha": "^3.1.2"
    },
    "peerDependencies": {
        "eslint": "^4.0.0"
    },
    "engines": {
        "node": ">=4.0.0"
